    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="keynotes" data-view="Meter" data-title="Keynotes (day 1)" style="background-color:#231F20;" data-min="0" data-max="600"></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="workshops" data-view="Meter" data-title="Workshops" style="background-color:#231F20;" data-min="0" data-max="200"></div>
