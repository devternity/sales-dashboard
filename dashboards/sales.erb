    <li data-row="1" data-col="2" data-sizex="3" data-sizey="2">
      <div data-id="tickets" data-view="List" data-unordered="true" style="background-color:#2EA946;" data-title="Tickets purchased" data-moreinfo="# of tickets purchased"></div>
